{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "Webpack 4 Boilerplate",
  "engines": {
    "node": "12.x.x",
    "npm": "6.x.x"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:stefanfrede/webpack-boilerplate.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": {
    "name": "Stefan Frede",
    "email": "contact@frede.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stefanfrede/webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/stefanfrede/webpack-boilerplate",
  "browserslist": [
    "defaults"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "sideEffects": false,
  "scripts": {
    "test": "echo \"This template does not include a test runner by default.\" && exit 1",
    "start": "nodemon --watch webpack.* --exec \"wp --mode development\"",
    "build": "wp --mode production"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "babel-eslint": "^10.1.0",
    "browserslist": "^4.14.4",
    "css-loader": "^4.3.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "glob": "^7.1.6",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "mini-css-extract-plugin": "^0.11.2",
    "mini-html-webpack-plugin": "^3.0.7",
    "nodemon": "^2.0.4",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "purgecss-webpack-plugin": "^3.0.0",
    "stylelint": "^13.7.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-order": "^4.1.0",
    "tailwindcss": "^1.8.10",
    "webpack": "^4.44.2",
    "webpack-merge": "^5.1.4",
    "webpack-nano": "^1.0.0",
    "webpack-plugin-serve": "^1.0.1",
    "webpackbar": "^4.0.0"
  },
  "dependencies": {}
}
